{
  "name": "dropbox",
  "oauth2": {
    "client_id": "DROPBOX_CLIENT_ID",
    "client_secret": "DROPBOX_CLIENT_SECRET",
    "auth_url": "https://www.dropbox.com/oauth2/authorize",
    "token_url": "https://api.dropboxapi.com/oauth2/token",
    "redirect_uri": "http://localhost:8083/oauth2/callback",
    "scopes": [
      "account_info.read",
      "files.metadata.read"
    ],
    "user_info_url": "https://api.dropboxapi.com/2/users/get_current_account"
  },
  "mappings": [
    {
      "field_key": "provider_user_id",
      "json_path": "account_id",
      "type": "string"
    },
    {
      "field_key": "display_name",
      "json_path": "name.display_name",
      "type": "string"
    },
    {
      "field_key": "email",
      "json_path": "email",
      "type": "string"
    },
    {
      "field_key": "avatar_url",
      "json_path": "profile_photo_url",
      "type": "string"
    }
  ]
}
