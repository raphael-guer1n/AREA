        {
          "name": "selected",
          "type": "boolean",
          "label": "Selected",
          "required": false,
          "default": "false"
        }
      ],
      "method": "POST",
      "bodyType": "json",
      "body_struct": [
        {
          "path": "id",
          "type": "text",
          "value": "{{calendarId}}"
        },
        {
          "path": "hidden",
          "type": "boolean",
          "value": "{{hidden}}"
        },
        {
