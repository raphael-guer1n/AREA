      "fields": [
        {
          "name": "calendarId",
          "type": "text",
          "label": "Calendar",
          "required": true,
          "default": "primary"
        }
      ],
      "method": "POST",
      "bodyType": "json",
      "body_struct": []
    },
    {
      "title": "create_calendar_list",
      "label": "Create Calendar List",
      "url": "https://www.googleapis.com/calendar/v3/users/me/calendarList",
